    "uglifyjs-webpack-plugin": "^2.1.1",
    "uppercamelcase": "^1.1.0",
    "url-loader": "0.6.2",
    "vue": "2.5.21",
    "vue-class-component": "^7.1.0",
    "vue-echarts": "^4.0.2",
    "vue-jest": "^3.0.5",
    "vue-loader": "^15.2.4",
    "vue-markdown-loader": "^2.4.1",
    "vue-router": "3.0.2",
    "vue-template-compiler": "2.5.21",
    "webpack": "4.29.6",
    "webpack-cli": "3.2.3",
    "webpack-dev-server": "3.2.1",
    "webpack-merge": "4.2.1",
    "webpack-node-externals": "^1.7.2"
  },
  "engines": {
    "node": ">= 4.0.0",
    "npm": ">= 3.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "lint-staged": {
    "**/*.{js,ts,vue}": "eslint"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "author": "HDVON",
  "license": "MIT"
}
